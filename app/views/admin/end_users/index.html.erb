 <div class="col-md-8 offset-md-1">
      <h2>会員一覧</h2>
      <table class="table table-hover table-inverse">
       <thead>
        <tr>
          <th>会員ID</th>
          <th>氏名</th>
          <th>メールアドレス</th>
          <th>ステータス</th>
        </tr>
      </thead>
       <% @end_users.each do |end_user| %>
      <tbody>
       <tr>
        <td><%= end_user.end_user_id %> </td>
        <td><%= link_to end_user.name, admin_end_user_path(end_user) %></td>
        <td><%= end_user.email %></td>
        <td><%= end_user.is_deleted %></td>
       </tr>
      </tbody>
      <% end %>
      </table>
    </div>
