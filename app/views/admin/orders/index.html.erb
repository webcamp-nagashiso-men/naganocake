<div class="col-md-8 offset-md-1">
      <h4>注文履歴一覧</h4>
      <table class="table table-hover table-inverse">
       <thead>
        <tr>
          <th>購入日時</th>
          <th>購入者</th>
          <th>注文個数</th>
          <th>注文ステータス</th>
        </tr>
      </thead>

      <tbody>
       <tr>
        <% @orders.each do |order| %>
        <td><%= order.created_at %></td>
        <% end %>

        <% @end_users.each do |end_user| %>
        <td><%= link_to end_user.name_last, admin_end_user_path(end_user) %><%= link_to end_user.name_first, admin_end_user_path(end_user) %></td>
        <% end %>

        <% @order_products.each do |order_product| %>
        <td><%= order_products.quantity %></td>
        <% end %>

        <% @orders.each do |order| %>
        <td><%= order.order_status %></td>
        <% end %>

       </tr>
      </tbody>
      <% end %>
      </table>
    </div>
